# Configuración de correo IMAP
IMAP_SERVER=imap.hostinger.com
IMAP_PORT=993
IMAP_USER=tu_correo@dominio.com
IMAP_PASSWORD=tu_contraseña

# Configuración de Supabase
SUPABASE_URL=https://tu-proyecto.supabase.co
SUPABASE_KEY=tu_clave_api
TABLE_NAME=catFacturas

# Configuración del procesador
POLLING_INTERVAL=60
POLLING_INTERVAL_IDLE=300
LOG_LEVEL=INFO

# Configuración de horarios
# SCHEDULE_ENABLED: true para activar horarios, false para funcionamiento 24/7
SCHEDULE_ENABLED=true
# SCHEDULE_START_TIME: Hora de inicio (formato HH:MM)
SCHEDULE_START_TIME=09:00
# SCHEDULE_END_TIME: Hora de fin (formato HH:MM)
SCHEDULE_END_TIME=18:00
# SCHEDULE_DAYS: Días de la semana (1=Lunes, 2=Martes, 3=Miércoles, 4=Jueves, 5=Viernes, 6=Sábado, 7=Domingo)
SCHEDULE_DAYS=1,2,3,4,5
# SCHEDULE_TIMEZONE: Zona horaria
SCHEDULE_TIMEZONE=America/Mexico_City

# Nota: Copiar este archivo a .env y configurar con los valores reales